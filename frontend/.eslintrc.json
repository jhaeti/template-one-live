{
	"env": {
		"browser": true,
		"es2021": true
	},
	"plugins": ["react-redux"],
	"extends": [
		"airbnb-base",
		"eslint:recommended",
		"plugin:react-redux/recommended",
		"next/core-web-vitals",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"rules": {
		"no-shadow": "off",
		"class-methods-use-this": "off",
		"no-underscore-dangle": "off",
		"default-param-last": "off",
		"react-redux/prefer-separate-component-file": "off",
		"react-hooks/exhaustive-deps": "off"
	}
}
