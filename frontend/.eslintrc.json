{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"airbnb-base",
		"eslint:recommended",
		"next/core-web-vitals",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"rules": {
		"no-shadow": "off",
		"class-methods-use-this": "off",
		"no-underscore-dangle": "off"
	}
}
